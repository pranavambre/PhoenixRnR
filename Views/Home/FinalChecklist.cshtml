
@{
    ViewBag.Title = "FinalChecklist";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/Content/finalChecklist.css" rel="stylesheet" />
<link href="~/Content/py.css" rel="stylesheet" />


<div class="block-31" style="position: relative;">
    <div class="owl-carousel loop-block-31 ">
        <div class="block-30 block-30-sm item" style="background-image: url('../../images/finalchecklist1.jpg'); background-position: 0% 0px;" data-stellar-background-ratio="0.5">
            <div class="container">
                <div class="row align-items-center justify-content-center text-center">
                    <div class="col-md-7">
                        <h2 class="heading mb-5">Get your final final checklist.</h2>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="container">
    <div class="checklist" id="content">
        <section>
            <header>Final Checklist</header>

            <ul>
                <li>
                    <input type="checkbox" name="item_0" id="item_0"><label for="item_0">Remove doormats from around the house.</label><span class="input"><span class="check"></span></span>
                </li>

                <li>
                    <input type="checkbox" name="item_1" id="item_1"><label for="item_1">Move combustible outdoor furniture and other items from decks</label><span class="input"><span class="check"></span></span>
                </li>

                <li>
                    <input type="checkbox" name="item_2" id="item_2"><label for="item_2">Move pot plants a few meters from the home</label><span class="input"><span class="check"></span></span>
                </li>

                <li>
                    <input type="checkbox" name="item_3" id="item_3"><label for="item_3">As the electricity might not be available to move the cars out of the garages with electric doors the night before.</label><span class="input"><span class="check"></span></span>
                </li>

                <li>
                    <input type="checkbox" name="item_99" id="item_99"><label for="item_99">Check and remove anything that could burn easily from around the house, on decks and verandahs areas.</label><span class="input"><span class="check"></span></span>
                </li>

                <li>
                    <input type="checkbox" name="item_4" id="item_4"><label for="item_4">Turn off the gas supply.</label><span class="input"><span class="check"></span></span>
                </li>

                <li>
                    <input type="checkbox" name="item_6" id="item_6"><label for="item_6">Block your downpipes and partially fill the gutters with water.</label><span class="input"><span class="check"></span></span>
                </li>

                <li>
                    <input type="checkbox" name="item_7" id="item_7"><label for="item_7">Close and lock all windows and doors.</label><span class="input"><span class="check"></span></span>
                </li>

                <li>
                    <input type="checkbox" name="item_8" id="item_8"><label for="item_8">Leave the front or access gate open, unless this will mean large animals or livestock will be released on to roads.</label><span class="input"><span class="check"></span></span>
                </li>

                <li>
                    <input type="checkbox" name="item_9" id="item_9"><label for="item_9">Make sure everyone is wearing protective clothing long pants, long sleeve shirts, and sturdy shoes such as leather boots. Clothes should be loose-fitting and made from natural fibers such as pure wool, heavy cotton drill, or denim avoid wearing synthetic.</label><span class="input"><span class="check"></span></span>
                </li>

                <li>
                    <input type="checkbox" name="item_11" id="item_11"><label for="item_11">Inform the neighbors about the leaving</label><span class="input"><span class="check"></span></span>
                </li>

                <li>
                    <input type="checkbox" name="item_12" id="item_12"><label for="item_12">Check the emergency kit is on an easy access</label><span class="input"><span class="check"></span></span>
                </li>

                <li>
                    <input type="checkbox" name="item_12" id="item_13"><label for="item_13">Stay updated about the fire information</label><span class="input"><span class="check"></span></span>
                </li>

                <li>
                    <input type="checkbox" name="item_12" id="item_14"><label for="item_14">Confirm the primary destination and back-up destination</label><span class="input"><span class="check"></span></span>
                </li>
            </ul>
           

        </section>
        <div class="text-center buttons-checklist">
            <button id="submit" class="download">Export to  PDF</button>
            <div class="alerts">
                <h1>Before leaving check alerts and warnings.</h1>
                <a class="download" href="/Home/GetYourChecklist">Back</a>
                <a class="download" href="/Home/Map">Alert Map</a>
            </div>
        </div>

    </div>
    <!--checklist-->

</div>
<script type="text/javascript" src="~/Scripts/finalchecklist.js"></script>
@*<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>*@
<script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.3.2/jspdf.debug.js"></script>
<script src="~/Scripts/jquery-3.3.1.min.js" type="text/javascript"></script>

<script id="rendered-js">
    var doc = new jsPDF();
    var specialElementHandlers = {
        '#editor': function (element, renderer) {
            return true;
        }
    };

    $('#submit').click(function () {
        doc.fromHTML($('#content').html(), 15, 15, {
            'width': 190,
            'elementHandlers': specialElementHandlers
        });

        doc.save('sample-page.pdf');
    });
//# sourceURL=pen.js
</script>
<script type="text/javascript" src="~/Scripts/finalchecklist.js"></script>
